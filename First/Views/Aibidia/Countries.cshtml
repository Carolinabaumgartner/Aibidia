@model IList<First.Storage.Entities.Country>

<html>
<h1>Countries</h1>
<div>

    <table class="table-bordered">
        <thead>
            <tr>
             
                <th>Country</th>
                <th>Continent</th>

            </tr>
        </thead>
        <tbody>

            @foreach (var country in Model)
            {
                <tr>
                    <td>@country.Name</td>
                    <td>@country.Continent.Name</td>
                    

                </tr>
            }
        </tbody>
    </table>
</div>
</html>